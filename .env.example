# AWS Bedrock Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
# Or use AWS profile instead:
# AWS_PROFILE=default

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000

# Model Configuration (Bedrock model IDs)
# Use regional inference profile format (e.g., us.anthropic.claude-sonnet-4-5-v1:0)
DEFAULT_MODEL=us.anthropic.claude-sonnet-4-5-20250929-v1:0

# MCP Server URLs (if running separately)
MATERIALS_MCP_URL=stdio://localhost:5001
PERMITTING_MCP_URL=stdio://localhost:5002

# Project Settings
MAX_PARALLEL_TASKS=3
TASK_TIMEOUT_SECONDS=300

# Logging
LOG_LEVEL=INFO
